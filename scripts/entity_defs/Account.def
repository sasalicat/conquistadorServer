		<onHallReady>
			<Exposed/>
		</onHallReady>
		<changeNickName>
			<Arg>	UNICODE	</Arg>
			<Exposed/>
		</changeNickName>
		<createRoom>
			<Arg>	UNICODE	</Arg>
			<Arg>	INT8	</Arg>
			<Arg>	ARRAY <of> INT8 </of>	</Arg>
			<Exposed/>
		</createRoom>
		<enterRoomReq>
			<Arg>	INT8	</Arg>
