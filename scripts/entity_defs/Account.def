<root>
	<Properties>
		<nickName>
			<Type>	UNICODE		</Type>
			<Flags>	BASE	</Flags>
			<Default>	None	</Default>
			<Persistent>	true	</Persistent>
		</nickName>
	</Properties>

	<ClientMethods>
		<updateNum>
			<Arg>	INT32	</Arg>
		</updateNum>
		<reqHallReady>
			<Arg>	UNICODE	</Arg>
		</reqHallReady>
		<updateRoom>
			<Arg>	RoomInfo	</Arg>
		</updateRoom>
		<getRoomList>
			<Arg>	RoomInfoList	</Arg>
		</getRoomList>
		<InitRoomInfo>
			<Arg>	PlayerInRoomList	</Arg>
