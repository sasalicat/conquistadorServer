<root>
	<Properties>
		<nickName>
			<Type>	UNICODE		</Type>
			<Flags>	BASE	</Flags>
			<Default>	None	</Default>
			<Persistent>	true	</Persistent>
		</nickName>
		<InRoom>
			<Type>	INT8		</Type>
			<Flags>	BASE_AND_CLIENT	</Flags>
			<Default>	-1	</Default>
			<Persistent>	false	</Persistent>
		</InRoom>
		<RoomNo>
			<Type>	INT8		</Type>
			<Flags>	BASE	</Flags>
			<Default>	-1	</Default>
			<Persistent>	false	</Persistent>
		</RoomNo>
		<RoleList>
			<Type>	ARRAY <of> RoleData </of>		</Type>
			<Flags>	BASE_AND_CLIENT	</Flags>
			<Default>	None	</Default>
			<Persistent>	true	</Persistent>
